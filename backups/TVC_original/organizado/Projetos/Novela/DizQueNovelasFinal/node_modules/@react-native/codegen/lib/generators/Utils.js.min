"use strict";function capitalize(t){return t.charAt(0).toUpperCase()+t.slice(1)}function indent(t,e){return t.split("\n").map((t,n)=>{if(0===t.length||0===n)return t;return new Array(e+1).join(" ")+t}).join("\n")}function toPascalCase(t){return 0===t.length?t:t[0].toUpperCase()+t.slice(1)}function toSafeCppString(t){return t.split("-").map(toPascalCase).join("")}function getEnumName(t,e){return`${t}${toSafeCppString(e)}`}module.exports={capitalize:capitalize,indent:indent,toPascalCase:toPascalCase,toSafeCppString:toSafeCppString,getEnumName:getEnumName};