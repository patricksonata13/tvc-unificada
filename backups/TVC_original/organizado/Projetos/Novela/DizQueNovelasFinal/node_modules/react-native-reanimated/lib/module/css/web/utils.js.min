"use strict";import{hasSuffix,maybeAddSuffix,processColor,ReanimatedError}from"../../common";import{CubicBezierEasing,LinearEasing,StepsEasing}from"../easing";import{kebabizeCamelCase}from"../utils";export function maybeAddSuffixes(i,e,r){return e in i?i[e].map(i=>maybeAddSuffix(i,r)):[]}function easingMapper(i){if("string"==typeof i)return i;if(i instanceof StepsEasing)return`steps(${i.stepsNumber}, ${kebabizeCamelCase(i.modifier)})`;if(i instanceof CubicBezierEasing)return`cubic-bezier(${i.x1}, ${i.y1}, ${i.x2}, ${i.y2})`;if(i instanceof LinearEasing){return`linear(${i.points.map(i=>Array.isArray(i)?i.join(" "):i).join(", ")})`}throw new ReanimatedError(`Invalid timing function ${i.toString()}`)}export function parseTimingFunction(i){return Array.isArray(i)?i.map(easingMapper).join(", "):easingMapper(i)}export function parseDimensionValue(i){if("object"!=typeof i)return"string"==typeof i||hasSuffix(i)?i:`${i}px`}export function opacifyColor(i,e){const r=processColor(i);if(null==r)return null;return`rgba(${r>>16&255}, ${r>>8&255}, ${255&r}, ${(r>>24&255)/255*e})`}