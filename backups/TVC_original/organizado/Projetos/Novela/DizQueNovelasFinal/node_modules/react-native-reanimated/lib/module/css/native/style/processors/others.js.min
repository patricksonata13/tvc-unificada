"use strict";import{ReanimatedError}from"../../../../common";export const ERROR_MESSAGES={unsupportedAspectRatio:t=>`Unsupported aspect ratio: ${t}. Expected a number or a string in "a/b" format.`};export const processAspectRatio=t=>{if("number"==typeof t||!isNaN(+t))return+t;if("string"==typeof t){const o=t.split("/");if(2===o.length){const t=parseFloat(o[0]),r=parseFloat(o[1]);if(!isNaN(t)&&!isNaN(r)&&0!==r)return t/r}}throw new ReanimatedError(ERROR_MESSAGES.unsupportedAspectRatio(t))};export const processGap=t=>({rowGap:t,columnGap:t});