"use strict";import{ReanimatedError}from"../../common";export const ERROR_MESSAGES={invalidCoordinate:(i,e)=>`Invalid ${i} coordinate for cubic bezier easing point, it should be a number between 0 and 1, received ${e}`};export class CubicBezierEasing{static easingName="cubicBezier";constructor(i,e,t,r){if(i<0||i>1)throw new ReanimatedError(ERROR_MESSAGES.invalidCoordinate("x1",i));if(t<0||t>1)throw new ReanimatedError(ERROR_MESSAGES.invalidCoordinate("x2",t));this.x1=i,this.y1=e,this.x2=t,this.y2=r}toString(){return`${CubicBezierEasing.easingName}(${this.x1}, ${this.y1}, ${this.x2}, ${this.y2})`}normalize(){return{name:CubicBezierEasing.easingName,x1:this.x1,y1:this.y1,x2:this.x2,y2:this.y2}}}