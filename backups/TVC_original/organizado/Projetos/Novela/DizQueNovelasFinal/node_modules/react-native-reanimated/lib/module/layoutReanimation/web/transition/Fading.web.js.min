"use strict";export function FadingTransition(a,t){const{translateX:s,translateY:r,scaleX:n,scaleY:e}=t;return{name:a,style:{0:{opacity:1,transform:[{translateX:`${s}px`,translateY:`${r}px`,scale:`${n},${e}`}]},20:{opacity:0,transform:[{translateX:`${s}px`,translateY:`${r}px`,scale:`${n},${e}`}]},60:{opacity:0,transform:[{translateX:"0px",translateY:"0px",scale:"1,1"}]},100:{opacity:1,transform:[{translateX:"0px",translateY:"0px",scale:"1,1"}]}},duration:300}}