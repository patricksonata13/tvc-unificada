"use strict";import{logger}from"../common";export function measure(e){const t=e();if(!t)return logger.warn(`The view with tag ${t} is not a valid argument for measure(). This may be because the view is not currently rendered, which may not be a bug (e.g. an off-screen FlatList item).`),null;const o=t.getBoundingClientRect();return{width:t.offsetWidth,height:t.offsetHeight,x:t.offsetLeft,y:t.offsetTop,pageX:o.left,pageY:o.top}}