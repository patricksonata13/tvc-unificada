"use strict";import{Easing}from"../../../Easing";export function JumpingTransition(a,t){const{translateX:s,translateY:n,scaleX:r,scaleY:e}=t,l=Math.max(Math.abs(s),Math.abs(n))/2,i=n<=0?n-l:-n+l;return{name:a,style:{0:{transform:[{translateX:`${s}px`,translateY:`${n}px`,scale:`${r},${e}`}],easing:Easing.exp},50:{transform:[{translateX:s/2+"px",translateY:`${i}px`,scale:`${r},${e}`}]},100:{transform:[{translateX:"0px",translateY:"0px",scale:"1,1"}]}},duration:300}}